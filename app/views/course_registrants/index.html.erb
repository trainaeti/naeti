<h1>Listing course_registrants</h1>

<table>
  <tr>
    <th>Course</th>
    <th>User</th>
    <th>Status</th>
    <th>Registration date</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @course_registrants.each do |course_registrant| %>
  <tr>
    <td><%= course_registrant.course_id %></td>
    <td><%= course_registrant.user_id %></td>
    <td><%= course_registrant.status %></td>
    <td><%= course_registrant.registration_date %></td>
    <td><%= link_to 'Show', course_registrant %></td>
    <td><%= link_to 'Edit', edit_course_registrant_path(course_registrant) %></td>
    <td><%= link_to 'Destroy', course_registrant, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

