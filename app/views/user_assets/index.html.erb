<h1>Listing user_assets</h1>

<table>
  <tr>
    <th>User</th>
    <th>Title</th>
    <th>Description</th>
    <th>Asset file name</th>
    <th>Asset content type</th>
    <th>Asset file size</th>
    <th>Asset updated at</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @user_assets.each do |user_asset| %>
  <tr>
    <td><%= user_asset.user_id %></td>
    <td><%= user_asset.title %></td>
    <td><%= user_asset.description %></td>
    <td><%= user_asset.asset_file_name %></td>
    <td><%= user_asset.asset_content_type %></td>
    <td><%= user_asset.asset_file_size %></td>
    <td><%= user_asset.asset_updated_at %></td>
    <td><%= link_to 'Show', user_asset %></td>
    <td><%= link_to 'Edit', edit_user_asset_path(user_asset) %></td>
    <td><%= link_to 'Destroy', user_asset, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User asset', new_user_asset_path %>
