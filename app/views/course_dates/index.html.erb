<h1>Listing course_dates</h1>

<table>
  <tr>
    <th>Course</th>
    <th>From date</th>
    <th>To date</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @course_dates.each do |course_date| %>
  <tr>
    <td><%= course_date.course_id %></td>
    <td><%= course_date.from_date %></td>
    <td><%= course_date.to_date %></td>
    <td><%= link_to 'Show', course_date %></td>
    <td><%= link_to 'Edit', edit_course_date_path(course_date) %></td>
    <td><%= link_to 'Destroy', course_date, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

